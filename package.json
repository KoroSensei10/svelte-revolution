{
	"name": "svelte-revolution",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"ia": "cd ./ia_server && cargo run",
		"ia-watch": "cd ./ia_server && cargo-watch -x 'run'",
